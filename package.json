{
  "name": "kgengine",
  "version": "0.11.2",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./cameras": {
      "import": "./dist/es/cameras.mjs",
      "require": "./dist/cjs/cameras.cjs",
      "types": "./dist/types/cameras.d.ts"
    },
    "./engine": {
      "import": "./dist/es/engine.mjs",
      "require": "./dist/cjs/engine.cjs",
      "types": "./dist/types/engine.d.ts"
    },
    "./lighting": {
      "import": "./dist/es/lighting.mjs",
      "require": "./dist/cjs/lighting.cjs",
      "types": "./dist/types/lighting.d.ts"
    },
    "./objects": {
      "import": "./dist/es/objects.mjs",
      "require": "./dist/cjs/objects.cjs",
      "types": "./dist/types/objects.d.ts"
    },
    "./otherScripts": {
      "import": "./dist/es/otherScripts.mjs",
      "require": "./dist/cjs/otherScripts.cjs",
      "types": "./dist/types/otherScripts.d.ts"
    },
    "./playerActions": {
      "import": "./dist/es/playerActions.mjs",
      "require": "./dist/cjs/playerActions.cjs",
      "types": "./dist/types/playerActions.d.ts"
    },
    "./shaders": {
      "import": "./dist/es/shaders.mjs",
      "require": "./dist/cjs/shaders.cjs",
      "types": "./dist/types/shaders.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "three": "^0.174.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.12",
    "@types/three": "^0.174.0",
    "gh-pages": "^6.3.0",
    "typescript": "^5.8.2",
    "vite": "^5.4.1"
  }
}
